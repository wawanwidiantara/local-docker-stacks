# MLflow Configuration
# Copy this file to .env and modify as needed

# PostgreSQL connection (requires postgres-container running)
# First create the database:
# docker exec -it postgres-container psql -U postgres -c "CREATE DATABASE mlflow;"

POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
MLFLOW_DB=mlflow

# MLflow UI credentials (optional, for basic auth)
MLFLOW_USERNAME=admin
MLFLOW_PASSWORD=password

# Access MLflow UI at: http://localhost:5000
